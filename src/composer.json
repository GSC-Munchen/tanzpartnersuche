{
    "name": "g-s-c/tanzpartnersuche",
    "type": "typo3-cms-extension",
    "description": "A typo3 extension for our dancing club, used to find a dancing partner.",
    "authors": [
        {
            "name": "Peter-Benedikt von Niebelsch\u00fctz",
            "role": "Developer"
        },
        {
            "name": "Martin Arend",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "require-dev": {
        "typo3/testing-framework": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "GSC\\Tanzpartnersuche\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GSC\\Tanzpartnersuche\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/tanzpartnersuche": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "tanzpartnersuche"
        }
    }
}